{
  "name": "client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED='0' ADDRESS_HEADER=True-Client-IP vite dev --host",
    "build": "ADDRESS_HEADER=True-Client-IP vite build",
    "preview": "ADDRESS_HEADER=True-Client-IP vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest run --coverage",
    "test:unit:watch": "vitest -w",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --plugin-search-dir . --check .",
    "format:fix": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.2.3",
    "@sveltejs/kit": "^1.20.4",
